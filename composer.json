{
    "name": "asafov/deployer-web",
    "description": "REST API for deployer service",
    "type": "project",
    "require": {
        "php": "^7.2.5",
        "deployer/deployer": "dev-master",
        "illuminate/redis": "^7.15",
        "laravel/lumen-framework": "^7.0",
        "symfony/process": "^5.1",
        "longman/telegram-bot": "0.63.*"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.9.1",
        "laravel/homestead": "^10.8",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    }
}
